<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel>
		<table border="0" cellpadding="0" cellspacing="0">
			<tr>
				<td>
					<div class="ks-decorator-errorLabel" ui:field="errorLabel" />
					<div class="ks-decorator-warningLabel" ui:field="warningLabel" />
				</td>
			</tr>
			<tr>
				<td>
					<g:SimplePanel ui:field="contents" stylePrimaryName="ks-decorator-contents" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>

</ui:UiBinder>