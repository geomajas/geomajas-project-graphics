<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel>
		<table border="0" cellpadding="0" cellspacing="0">
			<tr>
				<td>
					<div class="txbx-decorator-errorLabel" ui:field="errorLabel" style="color:red" />
					<div class="txbx-decorator-warningLabel" ui:field="warningLabel" style="color:blue" />
				</td>
			</tr>
			<tr>
				<td>
					<g:SimplePanel ui:field="contents" stylePrimaryName="txbx-decorator-contents" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>

</ui:UiBinder>