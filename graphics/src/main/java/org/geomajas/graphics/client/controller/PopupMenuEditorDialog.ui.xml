<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:DialogBox>
		<g:caption></g:caption>
		<g:VerticalPanel>
			<g:SimplePanel ui:field="editorPanel" height="100%"/>
			<g:HorizontalPanel styleName="popup-buttons">
				<g:Button ui:field="okButton">Ok</g:Button>
				<g:Button ui:field="applyButton" >Apply</g:Button>
				<g:Button ui:field="undoButton" >Undo</g:Button>
				<g:Button ui:field="cancelButton" >Cancel</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>

</ui:UiBinder>