<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field='msg'
		type='org.geomajas.graphics.client.resource.GraphicsMessages' />
		
	<ui:with field='res'
		type='org.geomajas.graphics.client.resource.GraphicsResource' />

	<g:PopupPanel ui:field="popupPanel" animationEnabled="true">
		<g:HorizontalPanel>
			<g:HTMLPanel ui:field="iconChoiceTablePanel">
				<table>
					<tr class="iconRow">
						<td>
							<g:Label>
								<ui:text from="{msg.createIconChoicePopupChooseIcon}" />
							</g:Label>
						</td>
					</tr>

					<tr  class="iconRow">
						<td>
							<g:FlowPanel  addStyleNames="{res.css.iconsPanel}" ui:field="iconsPanel" />
						</td>
					</tr>

					<tr  class="markerRow iconRow">
						<td class="lineCell">
						</td>
					</tr>

					<tr class="markerRow">
						<td>
							<g:Label>
								<ui:text from="{msg.createIconChoicePopupChooseMarker}" />
							</g:Label>
						</td>
					</tr>
					<tr  class="markerRow">
						<td>
							<g:SimplePanel ui:field="markersPanel" />
						</td>
					</tr>
					<tr>
						<td>
							<g:Button ui:field="okButton">
								<ui:text from="{msg.createIconChoicePopupOkButton}" />
							</g:Button>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
			<g:SimplePanel ui:field="previewPanel" />
		</g:HorizontalPanel>
	</g:PopupPanel>

</ui:UiBinder>